<%= simple_form_for @workout do |f| %>

  <div class="form-inputs">
    <%= f.input :key, input_html: { class: 'span2' }%>
    <%= f.input :name %>
    <%= f.input :description, input_html: { class: 'span6' } %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn-primary' %>
  </div>

<% end %>
